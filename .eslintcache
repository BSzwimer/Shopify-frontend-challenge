  return (\n        <li key={nomination.imdbID}>\n          {nomination.Title} ({nomination.Year})\n        </li>\n      );\n    });\n  };\n\n  const onSubmit = () => {\n    setOpen(false);\n    props.nominations.map((nomination) => {\n      props.deleteNomination(nomination.imdbID);\n    });\n    alert.success(\"Shoppies Submitted!\");\n  };\n\n  if (props.nominations.length === 5) {\n    return (\n      <Modal\n        closeIcon\n        open={open}\n        onClose={() => setOpen(false)}\n        onOpen={() => setOpen(true)}\n      >\n        <Header content=\"Your Shoppies Nomination\" />\n        <Modal.Content>\n          <ol className=\"ui list\">{renderList()}</ol>\n          <h3>Ready to submit?</h3>\n        </Modal.Content>\n        <Modal.Actions>\n          <Button color=\"red\" onClick={() => setOpen(false)}>\n            <Icon name=\"remove\" /> No\n          </Button>\n          <Button color=\"green\" onClick={() => onSubmit()}>\n            <Icon name=\"checkmark\" /> Yes\n          </Button>\n        </Modal.Actions>\n      </Modal>\n    );\n  }\n  return null;\n};\n\nconst mapStateToProps = (state) => {\n  return {\n    nominations: Object.values(state.nominations),\n  };\n};\n\nexport default connect(mapStateToProps, { deleteNomination })(PopUp);\n","/Users/benjaminszwimer/Desktop/shopifyIntern/shopify/src/components/NominationItem.js",[],{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","severity":1,"message":"77","line":10,"column":6,"nodeType":"78","endLine":10,"endColumn":8,"suggestions":"79"},{"ruleId":"72","replacedBy":"80"},{"ruleId":"74","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":3,"column":1,"nodeType":"84","endLine":10,"endColumn":3},{"ruleId":"85","severity":1,"message":"86","line":17,"column":13,"nodeType":"87","endLine":20,"endColumn":15},{"ruleId":"88","severity":1,"message":"89","line":15,"column":37,"nodeType":"90","messageId":"91","endLine":15,"endColumn":39},{"ruleId":"